# Exchange API Keys
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here
COINBASE_API_KEY=your_coinbase_api_key_here
COINBASE_API_SECRET=your_coinbase_api_secret_here
COINBASE_API_PASSPHRASE=your_coinbase_passphrase_here

# WebSocket URLs
WS_URL=wss://stream.binance.com:9443
COINBASE_WS_URL=wss://ws-feed.exchange.coinbase.com

# Testnet Mode
USE_TESTNET=false

# Trading Pairs
TRADING_PAIR=BTC-USD
BINANCE_SYMBOL=BTCUSDT
MIN_SPREAD=0.1
TRADE_SIZE=0.01
MAX_POSITION=0.05

# Trade Parameters
TRADE_AMOUNT=0.001
BINANCE_FEE=0.001
COINBASE_FEE=0.005
SIMULATED_LATENCY=0.05
SLIPPAGE_RATE=0.0005

# Model
MODEL_PATH=models/arb_model.pkl

# API Settings
API_HOST=0.0.0.0
API_PORT=8000

# CORS Settings
CORS_ORIGINS=http://localhost:3000,https://your-frontend-domain.vercel.app

# ML Model Parameters
CONFIDENCE_THRESHOLD=0.75
WINDOW_SIZE=60
FEATURE_COLUMNS=price_diff,spread_pct,volume,volatility
